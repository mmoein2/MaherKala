  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
	<add key="ImageRoot" value="~/editor/"/>
    <add key="FilesRoot" value="~/files/"/>
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5.2" />
    <httpRuntime targetFramework="4.5.2" />
     <authentication mode="Forms">
      <forms cookieless="UseCookies" defaultUrl="~/Home/Index" loginUrl="~/User/Login" slidingExpiration="true" timeout="2880"
              ></forms>
    </authentication>

    
    
   <roleManager defaultProvider="RoleProviderManager" enabled="true">
      <providers>
        <clear/>
        <add name="RoleProviderManager" type="WebApplication1.MyRoleProvider.RoleProviderManager"/>
      </providers>
    </roleManager>

    <pages>
      <namespaces>
        <add namespace="System.Web.Helpers" />
        <add namespace="System.Web.Mvc" />
        <add namespace="System.Web.Mvc.Ajax" />
        <add namespace="System.Web.Mvc.Html" />
        <add namespace="System.Web.Routing" />
        <add namespace="System.Web.WebPages" />
      </namespaces>
    </pages>
      <customErrors mode="Off" />
  </system.web>
  
   <location path="Admin">
    <system.web>
      <authorization>
        <deny users="?"/>
        <deny roles="Member"/>
        <allow roles="Admin"/>
      </authorization>
    </system.web>
  </location>
  
   <location path="ckeditor">
      <system.web>
         <authorization>
           <deny users="?"/>
           <deny roles="Member"/>
           <allow roles="Admin"/>
         </authorization>
      </system.web>
   </location>
  
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <handlers>
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <remove name="OPTIONSVerbHandler" />
      <remove name="TRACEVerbHandler" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="*" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
  </system.webServer>

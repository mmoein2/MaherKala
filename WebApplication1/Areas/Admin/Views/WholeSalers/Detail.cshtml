
@{
    ViewBag.Title = "Detail";
    Layout = "~/Areas/Admin/Views/Shared/_Master.cshtml";
}


<div class="box box-primary">
    <div class="box-body">
        <div class="form-control">
            <label>نوع کاربر :</label>
            <span>@ViewBag.Data.GetType()</span>
        </div>
        <div class="form-control">
            <label>نام و نام خانوادگی :</label>
            <span>@ViewBag.Data.Fullname</span>
        </div>
        <div class="form-control" style="height:100%;min-height:30px">
            <label>آدرس :</label>
            <p>@ViewBag.Data.Address</p>
        </div>
        <div class="form-control">
            <label>کدپستی :</label>
            <span>@ViewBag.Data.PostalCode</span>
        </div>
        <div class="form-control">
            <label>تلفن :</label>
            <span>@ViewBag.Data.Phone</span>
        </div>
        <div class="form-control">
            <label>موبایل :</label>
            <span>@ViewBag.Data.Mobile</span>
        </div>
        <div class="form-control" style="height:100%">
            <label>شرایط و توضیحات :</label>
            <p>@ViewBag.Data.Desc</p>
        </div>
        <a href="/Admin/WholeSalers/Index" class="btn btn-primary" style="margin-top:20px"> بازگشت</a>

    </div>
</div>

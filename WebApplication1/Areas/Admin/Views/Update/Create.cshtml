
@{
    ViewBag.Title = "Create";
    Layout = "~/Areas/Admin/Views/Shared/_Master.cshtml";
}



<div style="color:red;font-weight:bold">
    @TempData["Error"]
</div>
<div class="box box-primary">

    <form enctype="multipart/form-data" method="post" action="/Update/Store" role="form">
        @Html.AntiForgeryToken()
        <!-- /.box-header -->

        <div class="box-body">
            <div class="form-group">
                <label for="exampleInputEmail1">کد آپدیت</label>
                <input min="0" name="VersionCode" type="number" class="form-control" id="">
            </div>
            <div class="form-group">
                <label for="exampleInputEmail1">نام آپدیت</label>
                <input value="" name="VersionName" type="text" class="form-control" id="">
            </div>
            <div class="form-group">
                <label for="exampleInputEmail1">تغییرات</label>
                <input value="" name="Desc" type="text" class="form-control" id="">
            </div>
            <div class="form-group">
                <label for="exampleInputEmail1">لینک</label>
                <input value="" name="Link" type="text" class="form-control" id="">
            </div>


        </div>
        <!-- /.box-body -->
        <div class="box-footer">
            <button type="submit" class="btn btn-success">ذخیره</button>

        </div>
    </form>
</div>